{
  "name": "todomvc-cycle",
  "version": "0.0.0",
  "author": "Andre Staltz",
  "repository": {
    "type": "git",
    "url": "git@github.com:staltz/todomvc-cycle.git"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "cyclejs": "0.21.1",
    "todomvc-app-css": "^1.0.0",
    "todomvc-common": "^1.0.1",
    "babel": "^5.1.13",
    "babelify": "^6.0.2",
    "browserify": "^10.0.0"
  },
  "scripts": {
    "build-debug": "mkdir -p js && browserify src/app.js -tg [ babelify --optional es3.propertyLiterals ] --outfile js/app.js",
    "uglify": "uglifyjs js/app.js -o js/app.min.js",
    "build": "npm run build-debug && npm run uglify"
  }
}
